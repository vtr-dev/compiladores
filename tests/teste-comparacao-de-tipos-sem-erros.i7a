funcao principal
  args
    inteiro a, b;
  fimargs

  literal aaa, bbb;

  se (a < b) entao
  escreva "a < b\n";
  senao se (a > b) entao
  escreva "a > b\n";
  senao
  escreva "a = b\n";
  fimse
fimfuncao

