 literal chute;
 inteiro cont;
 
 chute="k";
 se(char==chute) entao
    escreva "Acertei!";
 fimse
 se (cont < 0) entao
    escreva "cont < 0";
 fimse
 enquanto (cont > 0) faca
    escreva char;
